{
	"manifest_version": 2,
	"name": "Textshifter",
	"description": "Converting the text",
	"version": "1.0.0",
	"icons": {
		"128": "icon_128.png"
	},
	"content_scripts": [{
		"matches": ["*://*/*"],
		"js": ["index.js"]
	}],
	"browser_action": {
		"default_title": "Textshifter",
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"permissions": ["activeTab"]
}